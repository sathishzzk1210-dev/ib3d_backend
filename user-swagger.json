          "id",
          "user_name",
          "email_id",
          "mobile_no",
          "invited_ref_code",
          "referral_code",
          "email_verified",
          "dob",
          "profile_url",
          "password",
          "device_token",
          "gender"
        ]
      },
      "CreateStoreDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "My Store"
          },
          "image_url": {
            "type": "string",
            "example": "http://example.com/image.jpg"
          },
          "mobile_no": {
            "type": "string",
            "example": "1234567890"
          },
          "alternate_mobile_no": {
            "type": "string",
            "example": "1234567890"
          },
          "email": {
            "type": "string",
            "example": "store@example.com"
          },
          "category": {
            "type": "string",
            "example": "supermarket"
          },
          "street": {
            "type": "string",
            "example": "mount road"
          },
          "city": {
            "type": "string",
            "example": "Chennai"
          },
          "state": {
            "type": "string",
            "example": "Tamil Nadu"
          },
          "postal_code": {
            "type": "string",
            "example": "600001"
          },
          "country": {
            "type": "string",
            "example": "India"
          },
          "open_time": {
            "type": "string",
            "example": "09:00"
          },
          "close_time": {
            "type": "string",
            "example": "21:00"
          },
          "owner_id": {
            "type": "number",
            "example": 1
          },
          "latitude": {
            "type": "number",
            "example": 13.0827
          },
          "longitude": {
            "type": "number",
            "example": 80.2707
          }
        },
        "required": [
          "name",
          "image_url",
          "mobile_no",
          "alternate_mobile_no",
          "email",
          "category",
          "street",
          "city",
          "state",
          "postal_code",
          "country",
          "open_time",
          "close_time",
          "owner_id",
          "latitude",
          "longitude"
        ]
      },
