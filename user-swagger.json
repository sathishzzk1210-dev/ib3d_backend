        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Surprise Bag - Bakery"
          },
          "product_image": {
            "type": "string",
            "example": "https://i.pcmag.com/imagery/articles/03xdeDG4m4n0gJG3CbFNIgm-17..v1661444591.png"
          },
          "description": {
            "type": "string",
            "example": "A surprise bag of fresh bakery items including bread, pastries, and more."
          },
          "category": {
            "type": "string",
            "example": "Bakery"
          },
          "store": {
            "type": "number",
            "example": 1
          },
          "product_id": {
            "type": "number",
            "example": 1
          },
          "original_price": {
            "type": "number",
            "example": 15
          },
          "discounted_price": {
            "type": "number",
            "example": 5
          },
          "currency": {
            "type": "string",
            "example": "INR"
          },
          "quantity": {
            "type": "number",
            "example": 10
          },
          "pickup_start_time": {
            "format": "date-time",
            "type": "string",
            "example": "2024-11-22T14:00:00"
          },
          "pickup_end_time": {
            "format": "date-time",
            "type": "string",
            "example": "2024-11-22T18:00:00"
          }
        },
        "required": [
          "name",
          "product_image",
          "description",
          "category",
          "store",
          "product_id",
          "original_price",
          "discounted_price",
          "currency",
          "quantity",
          "pickup_start_time",
          "pickup_end_time"
        ]
      },
      "UpdateProductDto": {
        "type": "object",
        "properties": {}
