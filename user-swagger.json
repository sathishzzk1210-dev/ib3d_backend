        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "user_name": {
            "type": "string"
          },
          "email_id": {
            "type": "string"
          },
          "mobile_no": {
            "type": "string"
          },
          "invited_ref_code": {
            "type": "string"
          },
          "referral_code": {
            "type": "string"
          },
          "email_verified": {
            "type": "boolean"
          },
          "dob": {
            "format": "date-time",
            "type": "string"
          },
          "profile_url": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "device_token": {
            "type": "string"
          },
          "gender": {
            "type": "string"
          }
        },
